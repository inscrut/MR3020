
config wifi-device 'radio0'
	option type 'mac80211'
	option channel '11'
	option hwmode '11g'
	option path 'platform/ar933x_wmac'
	option htmode 'HT20'
    option txpower '15'
	option country 'RU'
	list ht_capab 'SHORT-GI-20'
	list ht_capab 'SHORT-GI-40'
	list ht_capab 'RX-STBC1'
	list ht_capab 'DSSS_CCK-40'

config wifi-iface
	option device 'radio0'
	option macaddr '00:0C:87:FF:FF:3F'
	option network 'wlan'
	option mode 'ap'
	option encryption 'psk2+tkip+aes'
	option hidden '0'
	option ssid 'FREE'
	option key '87654321'
	option disabled '1'

config wifi-iface
	option device 'radio0'
	option macaddr '00:0C:87:FF:FF:4F'
	option network 'stlan'
	option mode 'sta'
	option encryption 'psk2+tkip+aes'
	option ssid 'Flat'
	option key '12345678'
	option disabled '1'
